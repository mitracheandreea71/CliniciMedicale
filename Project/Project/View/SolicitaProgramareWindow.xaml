        <DatePicker x:Name="DatePicker" SelectedDate="{Binding DataProgramare}" DisplayDateStart="{x:Null}" 
            DisplayDateEnd="{x:Null}"  Margin="35,346,572,10"/>
        <TextBox x:Name="TxtBoxCNP" HorizontalAlignment="Left" Margin="98,187,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="130"/>
        <TextBlock x:Name="TxtBlockCNP" HorizontalAlignment="Left" Margin="35,187,0,0" TextWrapping="Wrap" Text="CNP" VerticalAlignment="Top" Width="26" Height="17"/>
        <TextBlock x:Name="TxtBlockNume" HorizontalAlignment="Left" Margin="35,107,0,0" TextWrapping="Wrap" Text="Nume" VerticalAlignment="Top" Width="58"/>
        <TextBlock x:Name="TxtBlockPrenume" HorizontalAlignment="Left" Margin="470,107,0,0" TextWrapping="Wrap" Text="Prenume" VerticalAlignment="Top" Height="19"/>
        <TextBlock x:Name="TxtBlockJud" HorizontalAlignment="Left" Margin="479,184,0,0" TextWrapping="Wrap" Text="Judet" VerticalAlignment="Top"/>
        <TextBlock x:Name="TxtBlockAdr" HorizontalAlignment="Left" Margin="35,270,0,0" TextWrapping="Wrap" Text="Adresa" VerticalAlignment="Top"/>
        <TextBox x:Name="TxtBoxNume" HorizontalAlignment="Left" Margin="98,107,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="130" Height="16"/>
        <TextBox x:Name="TxtBoxPrenume" HorizontalAlignment="Left" Margin="590,105,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="139" TextChanged="TextBox_TextChanged"/>
        <TextBox x:Name="TxtBoxJud" HorizontalAlignment="Left" Margin="590,183,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="139"/>
        <TextBox x:Name="TxtBoxAdr" HorizontalAlignment="Left" Margin="98,268,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="130"/>
        <ComboBox Margin="418,353,226,38"/>
        <TextBlock x:Name="TxtBlockJud_Copy" HorizontalAlignment="Left" Margin="344,358,0,0" TextWrapping="Wrap" Text="Alege ora:" VerticalAlignment="Top" Height="22" Width="56"/>
    </Grid>
