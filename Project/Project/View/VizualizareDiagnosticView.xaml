<UserControl x:Class="Project.View.VizualizareDiagnosticView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Project.View"
             mc:Ignorable="d" 
              d:DesignHeight="900" d:DesignWidth="850">
    
    <Grid Background="White">
        <TextBlock HorizontalAlignment="Left" Height="23" Margin="21,18,0,0" TextWrapping="Wrap" Text="Unitatea sanitara: " VerticalAlignment="Top" Width="160" TextAlignment="Center" FontSize="16" FontFamily="Arial Black" Grid.ColumnSpan="2"/>

        <TextBlock HorizontalAlignment="Left" Height="23" Margin="21,41,0,0" TextWrapping="Wrap" Text="Localitatea: " VerticalAlignment="Top" Width="109" FontSize="16" FontFamily="Arial Black"/>
        <TextBlock HorizontalAlignment="Left" Height="23" Margin="21,64,0,0" TextWrapping="Wrap" Text="Judetul: " VerticalAlignment="Top" Width="77" FontSize="16" FontFamily="Arial Black"/>
        <TextBox HorizontalAlignment="Left" IsReadOnly="True" Margin="190,18,0,0" TextWrapping="Wrap" Text="{Binding NumeClinica}" BorderThickness="0,0,0,1" VerticalAlignment="Top" Width="643" Height="23" FontFamily="Arial" FontSize="16"/>
        <TextBox HorizontalAlignment="Left" IsReadOnly="True" Margin="136,41,0,0" TextWrapping="Wrap" Text="{Binding LocalitateClinica}" BorderThickness="0,0,0,1" VerticalAlignment="Top" Width="697" Height="23" FontFamily="Arial" FontSize="16"/>
        <TextBox HorizontalAlignment="Left" IsReadOnly="True" Margin="103,64,0,0" TextWrapping="Wrap" Text="{Binding JudetClinica}" BorderThickness="0,0,0,1" VerticalAlignment="Top" Width="730" Height="23" FontFamily="Arial" FontSize="16"/>
        <TextBlock HorizontalAlignment="Center" Height="32" Margin="0,136,0,0" TextWrapping="Wrap" Text="Fisa de consultatie" VerticalAlignment="Top" Width="318" FontSize="30" FontFamily="Arial Black"/>
        <TextBlock HorizontalAlignment="Left" Height="23" Margin="21,198,0,0" TextWrapping="Wrap" Text="Numele: " VerticalAlignment="Top" Width="77" TextAlignment="Center" FontSize="16" FontFamily="Arial Black"/>
        <TextBlock HorizontalAlignment="Left" Height="23" Margin="21,226,0,0" TextWrapping="Wrap" Text="Prenumele: " VerticalAlignment="Top" Width="102" TextAlignment="Center" FontSize="16" FontFamily="Arial Black"/>
        <TextBlock HorizontalAlignment="Left" Height="23" Margin="21,254,0,0" TextWrapping="Wrap" Text="Domiciliat: jud. " VerticalAlignment="Top" Width="142" TextAlignment="Center" FontSize="16" FontFamily="Arial Black"/>
        <TextBox HorizontalAlignment="Left" IsReadOnly="True" Margin="163,255,0,0" TextWrapping="Wrap" Text="{Binding JudetPacient}" BorderThickness="0,0,0,1" VerticalAlignment="Top" Width="190" Height="22" FontFamily="Arial" FontSize="16"/>
        <TextBox HorizontalAlignment="Left" IsReadOnly="True" Margin="100,199,0,0" TextWrapping="Wrap" Text="{Binding NumePacient}" BorderThickness="0,0,0,1" VerticalAlignment="Top" Width="736" Height="22" FontFamily="Arial" FontSize="16"/>
        <TextBox HorizontalAlignment="Left" IsReadOnly="True" Margin="125,227,0,0" TextWrapping="Wrap" Text="{Binding PrenumePacient}" BorderThickness="0,0,0,1" VerticalAlignment="Top" Width="711" Height="22" FontFamily="Arial" FontSize="16"/>
        <TextBlock HorizontalAlignment="Left"  Height="23" Margin="346,254,0,0" TextWrapping="Wrap" Text="Adesa " VerticalAlignment="Top" Width="79" TextAlignment="Center" FontSize="16" FontFamily="Arial Black"/>
        <TextBox HorizontalAlignment="Left"  IsReadOnly="True" Margin="430,255,0,0" TextWrapping="Wrap" Text="{Binding AdresaPacient}" BorderThickness="0,0,0,1" VerticalAlignment="Top" Width="406" Height="22" FontFamily="Arial" FontSize="16"/>
        <TextBlock HorizontalAlignment="Left" Height="24" Margin="21,426,0,0" TextWrapping="Wrap" Text="Tratament: " VerticalAlignment="Top" Width="102" TextAlignment="Center" FontSize="16" FontFamily="Arial Black"/>
        <TextBox HorizontalAlignment="Left"  IsReadOnly="True" Margin="132,426,0,0" TextWrapping="Wrap" Text="{Binding Tratament }" BorderThickness="0,0,0,1" VerticalAlignment="Top" Width="706" Height="306" FontFamily="Arial" FontSize="14"/>
        <TextBlock HorizontalAlignment="Left" Height="24" Margin="21,288,0,0" TextWrapping="Wrap" Text="Diagnostic: " VerticalAlignment="Top" Width="102" TextAlignment="Center" FontSize="16" FontFamily="Arial Black"/>
        <TextBox HorizontalAlignment="Left"  IsReadOnly="True" Margin="130,288,0,0" TextWrapping="Wrap" Text="{Binding Diagnostic}" BorderThickness="0,0,0,1" VerticalAlignment="Top" Width="706" Height="129" FontFamily="Arial" FontSize="14"/>
        <TextBlock HorizontalAlignment="Left" Height="24" Margin="25,743,0,0" TextWrapping="Wrap" Text="Observatii: " VerticalAlignment="Top" Width="102" TextAlignment="Center" FontSize="16" FontFamily="Arial Black"/>
        <TextBox HorizontalAlignment="Left"  IsReadOnly="True" Margin="134,743,0,0" TextWrapping="Wrap" Text="{Binding Observatii}" BorderThickness="0,0,0,1" VerticalAlignment="Top" Width="706" Height="102" FontFamily="Arial" FontSize="14"/>

    </Grid>
</UserControl>
